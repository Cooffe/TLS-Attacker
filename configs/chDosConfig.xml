<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<config>
	<highestProtocolVersion>DTLS12</highestProtocolVersion>
	<defaultSelectedProtocolVersion>DTLS12</defaultSelectedProtocolVersion>
	<workflowExecutorType>DTLS</workflowExecutorType>
    <defaultClientConnection>
        <transportHandlerType>UDP</transportHandlerType>
    </defaultClientConnection>

	<maxDtlsRetransmissions>0</maxDtlsRetransmissions>
	<finishWithCloseNotify>true</finishWithCloseNotify>
	<ignoreRetransmittedCcsInDtls>true</ignoreRetransmittedCcsInDtls>
	<dtlsCookieExchange>true</dtlsCookieExchange>
	<workflowTraceType>DYNAMIC_HELLO</workflowTraceType>
	
	<createIndividualTransportPackets>true</createIndividualTransportPackets>
	<dtlsMaximumFragmentLength >250</dtlsMaximumFragmentLength>
	<waitTime>1000</waitTime>
</config>